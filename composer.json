{
    "name": "danielgp/informator",
    "description": "Informations about environment served via web",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Popiniuc",
            "email": "danielpopiniuc@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-mysqli": "*",
        "inetsys/phpgettext":">=1.0.11",
        "danielgp/common-lib": ">=1.0.6",
        "piwik/device-detector": ">=2.8.0",
		"doctrine/cache": "*"
    },
    "autoload": {
        "psr-4": {
            "": "source/info/",
            "danielgp\\common_lib\\": "vendor/danielgp/common-lib/",
            "DeviceDetector\\" : "vendor/piwik/device-detector/"
        },
        "classmap": [
            "source/info/AppQueries.php",
            "source/info/Informator.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "archive": {
        "exclude": ["/nbproject", "/vendor", "/tests"]
    }
}
